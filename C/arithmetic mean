    #include <stdio.h>
    #include <stdbool.h>
     
    int main()
    {
      int array[30];
      int i=0;
      int j=0;
      int sum = 0;
      int exit=0;

      while (i<5) {
        printf("Enter number: ");
        scanf("%d", &array[i]);
        i++;
      }
      
      for (i = 0; i < 5; i++) {
        sum += array[i];
        }
      printf("%d", sum/i);
      return 0;
    }
